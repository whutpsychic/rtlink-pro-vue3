{
  "name": "rtlink-pro-vue3",
  "version": "0.1.0",
  "description": "本项目是适用于 vue3 框架的前端开源项目，瑞太智联自用的UI组件库。\r 要求 vue3 版本 >= 2.5.17。\r 本项目UI部分依赖于 element-plus。",
  "main": "./lib/main.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "element-plus": "^2.4.3"
  },
  "devDependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "highlight.js": "^11.9.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "pinia": "^2.0.28",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vitest": "^0.25.6",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whutpsychic/rtlink-pro-vue3.git"
  },
  "keywords": [
    "rtlink",
    "rtlink-ui",
    "rtlink-pro",
    "rtlink-ui-plus",
    "element-ui",
    "element-plus"
  ],
  "author": "zbc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/whutpsychic/rtlink-pro-vue3/issues"
  },
  "homepage": "https://github.com/whutpsychic/rtlink-pro-vue3#readme"
}
